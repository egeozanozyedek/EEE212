                       331     CONFIGURE_LCD:  ;THIS SUBROUTINE SENDS THE INITIALIZATION COMMANDS TO THE LCD
0205 7438              332             mov a,#38H      ;TWO LINES, 5X7 MATRIX
0207 511A              333             acall SEND_COMMAND
0209 740F              334             mov a,#0FH      ;DISPLAY ON, CURSOR BLINKING
020B 511A              335             acall SEND_COMMAND
020D 7406              336             mov a,#06H      ;INCREMENT CURSOR (SHIFT CURSOR TO RIGHT)
020F 511A              337             acall SEND_COMMAND
0211 7401              338             mov a,#01H      ;CLEAR DISPLAY SCREEN
0213 511A              339             acall SEND_COMMAND
0215 7480              340             mov a,#80H      ;FORCE CURSOR TO BEGINNING OF THE FIRST LINE
0217 511A              341             acall SEND_COMMAND
0219 22                342             ret
                       343
                       344
                       345
                       346     SEND_COMMAND:
021A F590              347             mov p1,a                ;THE COMMAND IS STORED IN A, SEND IT TO LCD
021C C2B5              348             clr p3.5                ;RS=0 BEFORE SENDING COMMAND
021E C2B6              349             clr p3.6                ;R/W=0 TO WRITE
0220 D2B7              350             setb p3.7       ;SEND A HIGH TO LOW SIGNAL TO ENABLE PIN
0222 5134              351             acall DELAY
0224 C2B7              352             clr p3.7
0226 22                353             ret
                       354
                       355
                       356     SEND_DATA:
0227 F590              357             mov p1,a                ;SEND THE DATA STORED IN A TO LCD
0229 D2B5              358             setb p3.5       ;RS=1 BEFORE SENDING DATA
022B C2B6              359             clr p3.6                ;R/W=0 TO WRITE
022D D2B7              360             setb p3.7       ;SEND A HIGH TO LOW SIGNAL TO ENABLE PIN
022F 5134              361             acall DELAY
0231 C2B7              362             clr p3.7
0233 22                363             ret
