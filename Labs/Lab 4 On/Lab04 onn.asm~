org 0
AGAIN: MOV DPTR,#TABLE
MOV R2,#40
BACK: CLR A
MOVC A,@A+DPTR
MOV P1,A ; sent to DAC
ACALL DELAY ; produce a freq.
INC DPTR
DJNZ R2,BACK
SJMP AGAIN


delay: 
	mov r5 ,#1
aca: 	djnz r5, aca
ret 
ORG 300h
TABLE: DB 128,138,147,157,167,176,185,194,202,210,218,224,231,236,241,245,249,251,253,255,255,255,253,251,249,245,241,236,231,224,218,210,202,194,185,176,167,157,147,138
end
